extends layout

block content
    if user
        a(href="/logout")= '注销'
        br
        a(href="/addUrl")= '增加链接'
        br
        a(href='/editTitle') 修改标题
    h1= meta.value
    ul
      each url in urls
        li
          a(href="#{url.link}")= url.content
          if user
            a(href="/delete/#{url._id}")= '删除'